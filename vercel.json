{
    "version": 2,
    "framework": "vite",
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/$1"
        },
        {
            "src": "/resources/(.*)",
            "dest": "/public/resources/$1"
        },
        {
            "src": "/(css|js|images|fonts|favicon)/(.*)",
            "dest": "/public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Huza Muhinzi",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://huza-muhinzi.vercel.app",
        "ASSET_URL": "https://huza-muhinzi.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp/views",
        "SSR_TEMP_PATH": "/tmp/ssr",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "NODE_PATH": "node"
    },
    "buildCommand": "npm install && npm run build && composer install --no-dev --optimize-autoloader && php artisan config:cache && php artisan route:cache && php artisan view:cache",
    "outputDirectory": "public",
    "devCommand": "php artisan serve & npm run dev"
}