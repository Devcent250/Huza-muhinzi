{
    "version": 2,
    "framework": null,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Huza Muhinzi",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://huza-muhinzi.vercel.app",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stack",
        "SESSION_DRIVER": "cookie",
        "SESSION_LIFETIME": "120",
        "QUEUE_DRIVER": "sync"
    }
}
